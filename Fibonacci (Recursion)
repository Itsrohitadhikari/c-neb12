//were starting recursion for Fibonacci here 

#include<stdio.h>

int fibo(int);

void main()

{

int i;

int n;

printf("How Many Data You Want to print: ");

scanf("%d",&n);

for(i = 0; i < n; i++)

{

printf("%d\n",fibo(i));

}

}

int fibo(int i)

{

if(i == 0)

{

return 0;

}

else if(i == 1)

{

return 1;

}

else

{

return(fibo(i-1)+fibo(i-2));

}

}
